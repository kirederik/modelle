<h1>Listing feedstock_stocks</h1>

<table>
  <tr>
    <th>Feedstock</th>
    <th>Quantity</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @feedstock_stocks.each do |feedstock_stock| %>
  <tr>
    <td><%= feedstock_stock.feedstock.name %></td>
    <td><%= feedstock_stock.quantity %></td>
    <td><%= link_to 'Show', feedstock_stock %></td>
    <td><%= link_to 'Edit', edit_feedstock_stock_path(feedstock_stock) %></td>
    <td><%= link_to 'Destroy', feedstock_stock, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Feedstock stock', new_feedstock_stock_path %>
